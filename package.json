    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "git-commit-msg-linter": "^4.2.1",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
